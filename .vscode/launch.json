    "version": "0.2.0",
    "configurations": [
        {
            "name": "Protractor",
            "type": "node",
            "request": "launch",
            "stopOnEntry": false,
            "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
            "args": [
                "${workspaceRoot}/e2e/protractor.conf.js"
            ],
            // "preLaunchTask": "builde2e",
            "sourceMaps": true,
            "outFiles": [ "../${workspaceRoot}/../out-tsc/e2e/**/*.js" ]
        }
    ]
}